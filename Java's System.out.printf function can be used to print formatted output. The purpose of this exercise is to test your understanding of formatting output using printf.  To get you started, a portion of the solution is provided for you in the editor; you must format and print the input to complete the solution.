import java.util.Scanner;
import java.io.PrintStream;
public class Solution {

    public static void main(String[] args) {
            Scanner sc=new Scanner(System.in);
            System.out.println("================================");
            for(int i=0;i<3;i++){
                String s1=sc.next();
                int x=sc.nextInt();
                //Complete this line
                
                System.out.format("%-15s%03d%n",s1,x);// %- ->indicates left aligned, 15s -> 15spaces from left , 03d -> 3digits if three digits not presnt then append 0 in front of the tolad no of digits present
            }
            System.out.println("================================");

    }
}

